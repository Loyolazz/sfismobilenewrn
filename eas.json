{
  "cli": {
    "version": ">=16.18.0",
    "appVersionSource": "remote"
  },
  "build": {
    "preview": {
      "distribution": "internal",
      "channel": "preview",
      "android": { "buildType": "apk" }
    },
    "production": {
      "distribution": "store",
      "channel": "production",
      "autoIncrement": true,
      "android": { "buildType": "app-bundle" }
    },
    "devclient": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "dev",
      "android": { "buildType": "apk" }
    }
  },
  "submit": {
    "production": {
      "android": { "track": "internal" }
    }
  }
}
